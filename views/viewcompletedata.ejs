<%- include('header') %>

<div class="view">
    <h1 class="heading"> Courses </h1>
    <div class="course">
        <h2>Course Name</h2>
        <h2>Course Faculty</h2>
        <h2>Course Code</h2>
        <h2>Course Department</h2>
    </div>
    <% courses.forEach(function(course){ %>

        <div class="course">

            <p>
                <%= course.name %>
            </p>
            <p>
                <%= course.faculty %>
            </p>
            <p>
                <%= course.code %>
            </p>
            <p>
                <%= course.dept %>
            </p>
        </div>

        <% }) %>
</div>

<%- include('footer') %>